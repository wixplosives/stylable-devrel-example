{
  "name": "delrel2",
  "version": "1.0.0",
  "description": "Stylable App",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "serve": "serve ./dist",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint",
    "start": "run-p start:*",
    "start:webpack": "webpack serve --open",
    "start:stylable": "stc --srcDir=src --outDir=st_types --no-cjs --dts --watch --preserveWatchOutput",
    "build:stylable": "stc --srcDir=src --outDir=st_types --no-cjs --dts"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stylable/cli": "^5.17.0",
    "@stylable/core": "^5.17.0",
    "@stylable/runtime": "^5.17.0",
    "@stylable/webpack-plugin": "^5.17.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
