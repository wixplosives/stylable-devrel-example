{
  "name": "delrel2",
  "version": "1.0.0",
  "description": "Stylable App",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "serve": "serve ./dist",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint",
    "start": "run-p start:*",
    "start:webpack": "webpack serve --open",
    "start:stylable": "stc --srcDir=src --outDir=st_types --no-cjs --dts --watch --preserveWatchOutput",
    "build:stylable": "stc --srcDir=src --outDir=st_types --no-cjs --dts"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@stylable/cli": "^4.12.0",
    "@stylable/core": "^4.12.0",
    "@stylable/runtime": "^4.12.0",
    "@stylable/webpack-plugin": "^4.12.0",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
